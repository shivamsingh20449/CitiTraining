	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int age = 18;
		try {
		if(age<=18)
		{
			throw new MyException();
		}
		}catch (MyException e) {
			// TODO: handle exception
			e.showCause();
		}

	}

}
